[alias]
  aa = add --all
	br = branch
	ci = commit
	co = checkout
	last = log -1 HEAD
  pure = pull --rebase
	sdiff = !git diff && git submodule foreach 'git diff'
	spush = push --recurse-submodules=on-demand
	st = status
  supdate = submodule update --remote --merge
[commit]
	gpgsign = true
  template = ~/.gitmessage
[core]
	editor = code --wait
[gpg]
	program = gpg
[user]
	name = Brendon Smith
	email = br3ndonland@protonmail.com
	signingkey = AC66384CFA8C69B0
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
