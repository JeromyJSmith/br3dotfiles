[alias]
aa = add --all
br = branch
bra = branch --all
brd = branch --delete
brD = branch --Delete
ci = commit
cia = commit --amend
co = checkout
cob = checkout -b
cod = checkout develop
com = checkout master
f = fetch
l = pull
last = log --graph --max-count=5 --show-signature
p = push
pure = pull --rebase
ra = remote add
rv = remote -v
sdiff = !git diff && git submodule foreach 'git diff'
spush = push --recurse-submodules=on-demand
st = status
stal = stash pop
stap = stash push
supdate = submodule update --remote --merge
[commit]
gpgsign = true
template = ~/.gitmessage
[core]
editor = codium --wait
[gpg]
program = gpg
[user]
name = Brendon Smith
email = br3ndonland@protonmail.com
signingkey = AC66384CFA8C69B0
[filter "lfs"]
clean = git-lfs clean -- %f
smudge = git-lfs smudge -- %f
process = git-lfs filter-process
required = true
