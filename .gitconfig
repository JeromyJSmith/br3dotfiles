[alias]
  aa = add --all
	br = branch
	ci = commit
	co = checkout
	last = log -1 HEAD
  pure = pull --rebase
	sdiff = !git diff && git submodule foreach 'git diff'
	spush = push --recurse-submodules=on-demand
	st = status
  supdate = submodule update --remote --merge
[commit]
	gpgsign = true
[core]
	editor = code --wait
[gpg]
	program = gpg
